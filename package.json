{
  "name": "glitchsquad",
  "version": "0.0.1",
  "description": "Turnbased squad action.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build.prod",
    "build.dev": "webpack --config ./webpack.build.config.js",
    "build.prod": "webpack --config ./webpack.prod.config.js",
    "start": "npm run build && node ./index.js",
    "frontend": "webpack-dev-server  --inline --content-base build",
    "watch.build": "watch 'npm run build.dev' ./src",
    "watch.public": "nodemon --watch public index.js",
    "watch": "concurrently 'npm run watch.build' 'npm run watch.public'"
  },
  "author": "mateusz curylo",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "watch": "^0.19.2",
    "webpack": "^1.13.1",
    "webpack-closure-compiler": "^2.0.2",
    "webpack-dev-server": "^1.14.1"
  },
   "engines": {
    "node": "6.*"
  },
  "dependencies": {
    "express": "^4.14.0",
    "socket.io": "^1.4.8"
  }
}
